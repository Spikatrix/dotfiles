{
    "telemetry.telemetryLevel": "off",
    "discord.detailsDebugging": "Squashing pesky bugs",
    "discord.detailsIdling": "Procrastinating",
    "discord.removeRemoteRepository": true,
    "workbench.colorTheme": "Pitch Black",
    "workbench.colorCustomizations": {
        "[Red]": {
            "editorCursor.foreground": "#ffff00",
            "terminalCursor.foreground": "#ffff00",
            "editor.selectionBackground": "#584e4e"
        },
        "[Pitch Black]": {
            "tab.activeBorder": "#ffff00",
            "menu.background": "#000000",
            "quickInput.background": "#000000",
            "sideBar.foreground": "#cccccc",
            "activityBarBadge.background": "#0000aa",
            "selection.background": "#666666",
            "editor.selectionBackground": "#333333",
            "editor.inactiveSelectionBackground": "#333333",
            "editor.lineHighlightBorder": "#00000000",
            "editor.lineHighlightBackground": "#111111",
            "editor.findMatchHighlightBackground": "#665500bb",
            "editor.findMatchHighlightBorder": "#00000000",
            "editor.findMatchBackground": "#333333",
            "editor.selectionHighlightBackground": "#223300",
            "editor.selectionHighlightBorder": "#00000000",
            "editor.findMatchBorder": "#aaaaaa",
            "editorSuggestWidget.background": "#000000",
            "editorHoverWidget.background": "#000000",
            "sideBarSectionHeader.background": "#000000",
            "statusBarItem.remoteBackground": "#000",
            "editorWhitespace.foreground": "#333333dd",
            "editorIndentGuide.background1": "#33333388",
            "editorIndentGuide.activeBackground1": "#333333bb"
        }
    },
    "git.confirmSync": false,
    "window.titleBarStyle": "custom",
    "diffEditor.renderSideBySide": false,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontLigatures": "'ccmp' off",
    "editor.fontFamily": "'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "scss.lint.validProperties": [
        "speak"
    ],
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.minimap.enabled": false,
    "terminal.integrated.tabs.enabled": true,
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "typescript.preferences.importModuleSpecifierEnding": "index",
    "editor.insertSpaces": false,
}